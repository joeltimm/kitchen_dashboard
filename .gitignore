cat > .gitignore <<'EOF'
# Python
__pycache__/
*.py[cod]
*.egg-info/
*.egg
*.pyo
*.pyd

# Virtual environments
venv/
.env
.pytest_cache/

# Auth tokens and credentials
*/*token*.json
*/*credentials*.json
*/*gmail_credentials*.json
*/*gmail_token*.json

# Logs
logs/
*.log
webhook_debug.log
debug.log

# Processed events tracking
processed_events.json

# Shell scripts (optional if private/local)
start_bot.sh

# macOS system files
.DS_Store
EOF
